= content_for :head_content do
  = render :partial => 'report_head'

= content_for :h1 do
  =h current_facility

= form_tag facility_reports_path do
  %ul.form
    %li
      %label Start Date
      = text_field_tag :date_start, params[:date_start] || human_datetime(@date_start, :date_only => true), :class => 'datepicker'
    %li
      %label End Date
      = text_field_tag :date_end, params[:date_end] || human_datetime(@date_end, :date_only => true), :class => 'datepicker'